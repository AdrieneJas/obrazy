<p style="color: green"><%= notice %></p>
<table>


<h1>Lista pinów</h1>

<thead>
<tr>
<th>Opis</th>
<th colspan="3"></th>
</tr>
</thead>

<% @pins.each do |pin| %>
<tr>
<td><%= pin.description %></td>
<td><%= link_to 'Wyświetl', pin %></td>
<% if pin.user == current_user %>
<td><%= link_to 'Edytuj', edit_pin_path(pin) %></td>
<td><%= link_to 'Usuń', pin, method: :delete, data: { confirm: 'Jesteś pewien?' } %></td>

<% end %>
</tr>
<% end %>

</table>

<%= link_to "Nowy pin", new_pin_path %>